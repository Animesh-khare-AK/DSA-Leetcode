class Solution {
    public int countPartitions(int[] nums) {
        int sum=0;
        int x=0;
        for(int i=0;i<nums.length;i++){
            sum = sum+nums[i];
        }
        int t=0;
        for(int i =0;i<nums.length-1;i++){
            t=t+nums[i];
            sum=sum-nums[i];
            if((t-sum)%2==0){
                x++;
            }

        }
        return x;
    }
}
